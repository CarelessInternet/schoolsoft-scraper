		"@babel/eslint-parser": "^7.15.7",
		"@types/jest": "^27.0.2",
		"@types/node": "^16.11.1",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"dotenv": "^10.0.0",
